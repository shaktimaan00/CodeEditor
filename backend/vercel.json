{
    "version": 2,
    "builds": [
        {
        "src": "./index.js",
        "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "handle": "filesystem"
        },
        {
            "src": "/users",
            "dest": "/routes/users.js"
        },
        {
            "src": "/(.*)",
            "dest": "/index.js"
        }
    ]
}